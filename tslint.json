{
  "extends": "tslint-config-airbnb",
  "rules": {
    "import-name": false,
    "object-curly-spacing": false,
    "variable-name": false,
    "max-line-length": [true, 300],
    "no-parameter-reassignment": false,
    "space-in-parens": false
  },
  "linterOptions": {
    "exclude": ["dist/**/*"]
  }
}
